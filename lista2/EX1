package lista2.EX1;
import java.util.Scanner;

public class EX1
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.print("Digite sua idade: ");
		int idade = scanner.nextInt();
		if(idade >=18) {
			System.out.print("Maior de idade");
		}
		scanner.close();
	}
}
